<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;
use Illuminate\Database\Eloquent\SoftDeletes;

class Products extends Model
{
    use HasFactory,SoftDeletes;

    protected $fillable = [
        'products_name',
        'description',
        'product_image',
        'category_id',
        'subcategory_id',
        'variations',
        'addon_id',
        'attribute_id',
        'product_price',
        'system_module_id',
        'product_tags',
        'brand_id',
        'store_id',
        'stock',
        'discount',
        'discount_type_id',
        'commission_amount'
    ];
}
