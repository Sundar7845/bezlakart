<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;
use Illuminate\Database\Eloquent\SoftDeletes;

class Products extends Model
{
    use HasFactory, SoftDeletes;

    protected $fillable = [
        'product_name',
        'description',
        'short_description',
        'product_image',
        'main_category_id',
        'category_id',
        'sub_category_id',
        'product_price',
        'system_module_id',
        'product_tags',
        'brand_id',
        'store_id',
        'product_stock',
        'discount',
        'discount_type_id',
        'commission_amount',
        'is_active',
        'created_by',
        'updated_by',
        'deleted_by'
    ];
}
